<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="src.mapper.datainputMapper">
    <insert id="addStudent" parameterType="src.pojo.Student">
        insert into test_info(stu_name, stu_id, distance)
        values (#{stuName}, #{stuID}, #{stuDistance});
    </insert>
</mapper>
